int main(int argc, char *argv[])
{
	Node * head = NULL;
	int number;
	do {
		scanf_s("%d", &number);
		if (number != -1) {
			add(head,number)
		}
	} while (number != -1);
	return 0;
}
void add(List* pList,int number)
{
	Node *p = (Node*)malloc(sizeof(Node));
	p->value;
	p->next = NULL;
	Node *last = pList->head;
	if (last) {
		while (last ->next) {
			last = last->next;
		}
	else {
		head = p;
		}
	}
}
